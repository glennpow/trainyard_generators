<%% title t(:create_object, :object => t(:<%= singular_name %>)) %>

<%% form_for(:<%= singular_name %>, :url => objects_path) do |f| %>
  <%= render :partial => 'edit', :locals => { :f => f, :<%= singular_name %> => @<%= singular_name %> } %>
  
  <%%= f.submit_with_cancel(:submit => t(:create)) %>
<%% end %>
